{
  "name": "@thebearingedge/mini-core",
  "version": "1.0.5",
  "description": "A small application core.",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/jshint && mocha --compilers js:babel/register",
    "tdd": "./node_modules/.bin/mocha -w --compilers js:babel/register",
    "cover": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha",
    "build": "./node_modules/.bin/babel index.js -o dist/index.js",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/mini-core.git"
  },
  "author": "Tim Davis",
  "keywords": [
    "ioc", "container", "di", "dependency injection", "core", "sandbox",
    "registry", "inversion of control"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebearingedge/mini-core/issues"
  },
  "homepage": "https://github.com/thebearingedge/mini-core#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-istanbul": "^0.4.0",
    "chai": "^3.4.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
}
