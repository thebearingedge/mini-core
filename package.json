{
  "name": "mini-core",
  "version": "3.0.5",
  "description": "A Dependency Injection framework.",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel/register",
    "cover": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha",
    "tdd": "./node_modules/.bin/mocha -w --compilers js:babel/register",
    "build": "./node_modules/.bin/babel mini-core.js -o dist/index.js",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/mini-core.git"
  },
  "author": "Tim Davis",
  "keywords": [
    "ioc",
    "container",
    "di",
    "dependency injection",
    "core",
    "sandbox",
    "registry",
    "inversion",
    "control"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebearingedge/mini-core/issues"
  },
  "homepage": "https://github.com/thebearingedge/mini-core#readme",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-istanbul": "0.4.0",
    "chai": "3.4.0",
    "coveralls": "2.11.4",
    "mocha": "2.3.3",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0"
  }
}
